<h2>Upload User</h2>

<div>

  <%= form_with(url: "/upload_file", method: "post", :html => { :multipart => true }) do %>

    <p>
      <%= label_tag(:csv_file, "Pick file:") %>
    </p>

    <p>
      <%= file_field_tag(:csv_file) %>
    </p>


    <%= submit_tag("Submit") %>


  <% end %>


</div>